<h1>Listing bars</h1>

<table>
  <tr>
    <th>Nameb</th>
    <th>Hoodb</th>
    <th>Kindb</th>
    <th>Dateb</th>
    <th>Patiob</th>
    <th>Costb</th>
    <th>Ambiguousb</th>
    <th>Addyb</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @bars.each do |bar| %>
  <tr>
    <td><%= bar.nameb %></td>
    <td><%= bar.hoodb %></td>
    <td><%= bar.kindb %></td>
    <td><%= bar.dateb %></td>
    <td><%= bar.patiob %></td>
    <td><%= bar.costb %></td>
    <td><%= bar.ambiguousb %></td>
    <td><%= bar.addyb %></td>
    <td><%= link_to 'Show', bar %></td>
    <td><%= link_to 'Edit', edit_bar_path(bar) %></td>
    <td><%= link_to 'Destroy', bar, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Bar', new_bar_path %>
